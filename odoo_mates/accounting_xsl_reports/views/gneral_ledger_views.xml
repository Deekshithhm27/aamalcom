<odoo>
    <record id="view_account_general_ledger_report_form_xlsx" model="ir.ui.view">
        <field name="name">account.report.general.ledger.form.xlsx</field>
        <field name="model">account.report.general.ledger</field>
        <field name="inherit_id" ref="accounting_pdf_reports.account_report_general_ledger_view"/>
        <field name="arch" type="xml">
            <xpath expr="//footer/button[@name='check_report']" position="after">
                <button name="print_xlsx_report" string="Export XLSX" type="object" class="btn-primary"/>
            </xpath>
        </field>
    </record>
</odoo>