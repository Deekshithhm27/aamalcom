<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
	    <record id="seq_hr_employee_change_request" model="ir.sequence">
	        <field name="name">Employee Change Request</field>
	        <field name="prefix">ECR/</field>
	        <field name="padding">5</field>
	        <field name="implementation">no_gap</field>
	        <field name="use_date_range" eval="False"/>
	        <field name="number_increment">1</field>
	        <field name="code">hr.employee.change.request</field>
	    </record>
	    <record id="ir_sequence_probation_extend_request" model="ir.sequence">
            <field name="name">Probation Extend Request</field>
        	<field name="code">hr.probation.extend.request</field>
            <field name="prefix">PER</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
        <record id="ir_sequence_end_of_contract" model="ir.sequence">
            <field name="name">End Of Contract</field>
        	<field name="code">hr.end.of.contract</field>
            <field name="prefix">EOC</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
                <!-- Sequence for Business Trip -->
        	    <record id="seq_medical_checklist" model="ir.sequence">
        	        <field name="name">Iqama Issuance Medical Sequence</field>
        	        <field name="code">hr.medical.blood.test</field>
        	        <field name="prefix">MBT</field>
        	        <field name="padding">4</field>
        	        <field name="number_next">1</field>
        	        <field name="number_increment">1</field>
        	    </record>
        	    <!-- Sequence for Business Trip -->
            <record id="seq_onboarding_checklist" model="ir.sequence">
                <field name="name">On Boarding Sequence</field>
                <field name="code">hr.onboarding.checklist</field>
                <field name="prefix">OBC</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for Business Trip -->
            <record id="seq_onboarding_process" model="ir.sequence">
                <field name="name">OnBoarding Process</field>
                <field name="code">hr.onboarding.process</field>
                <field name="prefix">OBP</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for Suspension Request -->
            <record id="seq_hr_suspension_request" model="ir.sequence">
                <field name="name">Suspension Request Sequence</field>
                <field name="code">hr.suspending</field>
                <field name="prefix">SE</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for Warning Request -->
            <record id="seq_hr_warning_request" model="ir.sequence">
                <field name="name">Warning Request Sequence</field>
                <field name="code">hr.warning</field>
                <field name="prefix">WL</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for Business Trip -->
            <record id="seq_hr_business_trip" model="ir.sequence">
                <field name="name">Business Trip Sequence</field>
                <field name="code">hr.business.trip</field>
                <field name="prefix">BT</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for Resignation -->
            <record id="seq_hr_resignation" model="ir.sequence">
                <field name="name">Resignation Sequence</field>
                <field name="code">hr.resignation</field>
                <field name="prefix">RR</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for Annual Request -->
            <record id="seq_hr_annual_request" model="ir.sequence">
                <field name="name">Annual Request Sequence</field>
                <field name="code">hr.annual.request</field>
                <field name="prefix">AR</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            <!-- Sequence for EOS Request -->
            <record id="seq_hr_end_of_service_request" model="ir.sequence">
                <field name="name">end of Service Sequence</field>
                <field name="code">hr.eos.request</field>
                <field name="prefix">EOS</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
            
            <!-- Sequence for EOS Request -->
            <record id="seq_hr_ere_of_service_request" model="ir.sequence">
                <field name="name">ERE Request</field>
                <field name="code">hr.exit.reentry</field>
                <field name="prefix">ERE</field>
                <field name="padding">4</field>
                <field name="number_next">1</field>
                <field name="number_increment">1</field>
            </record>
        
	</data>
</odoo>