<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data noupdate="1">
	    <record id="seq_hr_employee_change_request" model="ir.sequence">
	        <field name="name">Employee Change Request</field>
	        <field name="prefix">ECR/</field>
	        <field name="padding">5</field>
	        <field name="implementation">no_gap</field>
	        <field name="use_date_range" eval="False"/>
	        <field name="number_increment">1</field>
	        <field name="code">hr.employee.change.request</field>
	    </record>
	     <record id="ir_sequence_probation_extend_request" model="ir.sequence">
            <field name="name">Probation Extend Request</field>
        	<field name="code">hr.probation.extend.request</field>
            <field name="prefix">PER/%(year)s/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>
        
	</data>
</odoo>