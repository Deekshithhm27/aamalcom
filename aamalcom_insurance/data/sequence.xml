<odoo>
    <record id="seq_insurance_reimbursement" model="ir.sequence">
        <field name="name">Insurance Reimbursement</field>
        <field name="code">insurance.reimbursement</field>
        <field name="prefix">REIMB</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>
    <record id="seq_medical_insurance_deletion" model="ir.sequence">
      <field name="name">Medical Insurance Deletion</field>
      <field name="code">medical.insurance.deletion</field>
      <field name="prefix">MID/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
    <record id="seq_medical_insurance_invoice_details" model="ir.sequence">
      <field name="name">Medical Insurance Invoice Details</field>
      <field name="code">medical.insurance.invoice.details</field>
      <field name="prefix">MIID/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
    <record id="seq_medical_insurance_class_change" model="ir.sequence">
      <field name="name">Medical Insurance Upgrade/Downgrade</field>
      <field name="code">medical.insurance.class.change</field>
      <field name="prefix">MICC/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
    <record id="seq_life_insurance_enrollment" model="ir.sequence">
      <field name="name">Life Insurance Enrollment</field>
      <field name="code">life.insurance.enrollment</field>
      <field name="prefix">LE/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
    <record id="seq_life_insurance_deletion" model="ir.sequence">
      <field name="name">Life Insurance Deletion</field>
      <field name="code">life.insurance.deletion</field>
      <field name="prefix">LID/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
    <record id="seq_life_insurance_invoice_details" model="ir.sequence">
      <field name="name">Life Insurance Invoice Details</field>
      <field name="code">life.insurance.invoice.details</field>
      <field name="prefix">LIID/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
    <record id="seq_life_insurance_class_change" model="ir.sequence">
      <field name="name">Life Insurance Upgrade/Downgrade</field>
      <field name="code">life.insurance.class.change</field>
      <field name="prefix">LICC/</field>
      <field name="padding">5</field>
      <field name="number_next">1</field>
      <field name="number_increment">1</field>
    </record>
</odoo>
